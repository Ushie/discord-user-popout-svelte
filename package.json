{
	"name": "discord-user-popout-svelte",
	"description": "A Discord user pop-out component for SvelteKit",
	"version": "1.1.2",
	"author": {
		"name": "Ushie",
		"email": "ushiekane@gmail.com",
		"url": "https://ushie.dev/"
	},
	"bugs": {
		"url": "https://github.com/ushie/discord-user-popout-svelte/issues"
	},
	"repository": "github:ushie/discord-user-popout-svelte",
	"license": "LGPL-3.0-only",
	"keywords": [
		"discord",
		"popout",
		"user popout",
		"discord svelte"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.26.0",
		"@sveltejs/package": "^2.2.2",
		"publint": "^0.1.16",
		"svelte": "^4.2.2",
		"svelte-check": "^3.5.2",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.5.0"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"sass": "^1.69.4",
		"svelte-markdown": "^0.4.0",
		"svooltip": "^0.7.5"
	}
}